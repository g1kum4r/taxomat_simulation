{
  "name": "Start on Heroku: Python",
  "description": "A Tax Simulation Python app, which can easily be deployed to Heroku.",
  "image": "heroku/python",
  "repository": "https://github.com/g1kum4r/taxomat_simulation",
  "keywords": ["python", "flask" ],
  "addons": [ "heroku-mongodb" ],
  "env": {
    "SECRET_KEY": {
      "description": "The secret key for the Django application.",
      "generator": "secret"
    }
  },
  "environments": {
    "test": {
      "scripts": {
        "test-setup": "python manage.py collectstatic --noinput",
        "test": "python manage.py test"
      }
    }
  },
  "expo": {

  }
}